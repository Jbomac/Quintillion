{
  "name": "@quanty/schematics",
  "version": "1.0.0",
  "description": "Quanty schematics for discord bot development",
  "main": "dist/src/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "postbuild": "yarn copy:collection && yarn copy:templates",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "copy:collection": "cpx src/collection.json dist && cpx src/templates/**/schema.json dist/templates",
    "copy:templates": "cpx src/templates/**/files/**/*.* dist/templates",
    "test": "yarn build && jasmine dist/templates/**/*.test.js"
  },
  "peerDependencies": {
    "typescript": "^3.4.5 || ^4.3.5"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^13.1.1",
    "@angular-devkit/schematics": "^13.1.1"
  },
  "devDependencies": {
    "@types/jasmine": "~3.10.0",
    "@types/node": "^16.11.13",
    "cpx": "^1.5.0",
    "jasmine": "^3.5.0",
    "rimraf": "^3.0.2",
    "turbo": "1.0.19",
    "typescript": "^4.5.4"
  }
}
