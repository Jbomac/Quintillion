{
  "baseBranch": "origin/master",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "dashboard#start": {
      "dependsOn": ["^server#start"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "lib/**"],
      "cache": true
    },
    "lint": {
      "outputs": []
    },
    "test": {},
    "format": {}
  }
}
