{
  "name": "quanty-bot",
  "version": "0.1.0-dev",
  "description": "A powerful discord bot built to provide features for both moderators and users",
  "main": "scr/index.ts",
  "author": "Douglas Rocha <79188161+slashtp3@users.noreply.github.com>",
  "license": "MIT",
  "repository": "https://github.com/slashtp3/Quanty-Bot.git",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node-dev --watch -r dotenv/config src/index.ts",
    "debug": "ts-node-dev --watch --debug -r dotenv/config src/index.ts",
    "start": " cross-env PRODUCTION=true NODE_PATH=.dist node ./dist/src/index",
    "lint": "cross-env TIMING=1 eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "mongogen": "mtgen  ./quanty/database/schemas -o ./quanty/types/"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "better-erela.js-apple": "^0.1.0",
    "chalk": "4.1.2",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "erela.js": "^2.3.3",
    "erela.js-spotify": "^1.2.0",
    "glob": "^7.2.0",
    "mongoose": "^6.1.1",
    "ms": "^2.1.3",
    "nekos.life": "^2.0.7",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.4.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.13",
    "@types/node-fetch": "^3.0.3",
    "config": "*",
    "eslint": "^8.4.1",
    "mongoose-tsgen": "^8.4.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig": "*",
    "typescript": "^4.5.4"
  }
}
